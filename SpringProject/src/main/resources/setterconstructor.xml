<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="emp1" class="com.spring.di.com.emexo.spring.di.setterconstructor.Employee">
		<constructor-arg index="1" value="Regu" />
		<constructor-arg index="0" value="1" />
		<property name="age" value="25"></property>
		<property name="email" value="regu@gmail.com"></property>
	</bean>
	
	<bean id="emp2" class="com.spring.di.com.emexo.spring.di.setterconstructor.Employee">
		<constructor-arg index="1" value="Arun" />
		<constructor-arg index="0" value="2" />
		<property name="age" value="28"></property>
		<property name="email" value="arun@gmail.com"></property>
	</bean>

</beans>